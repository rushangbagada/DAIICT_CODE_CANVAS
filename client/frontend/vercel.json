{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://daiict-code-canvas-1.onrender.com/api/$1"
    },
    {
      "source": "/ml-api/(.*)",
      "destination": "https://daiict-code-canvas-1.onrender.com/ml-api/$1"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    },
    {
      "source": "/ml-api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
